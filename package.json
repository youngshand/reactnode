{
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">=5.0 <6",
    "npm": ">=3.3 <4"
  },
  "scripts": {
    "clean": "rm -rf lib",
    "watch-js": "babel src --watch --out-dir lib",
    "dev-server": "babel-node lib/server/webpack -c /lib/config/webpack.config.js",
    "test": "",
    "test:lint": "eslint src/* --ext .js --fix --cache-file",
    "server": "nodemon lib/server/server.js",
    "start": "if npm run test:lint; then npm run watch-js & npm run dev-server & npm run server; fi;",
    "build": "npm run clean && ./node_modules/babel-cli/bin/babel.js src --out-dir lib",
    "debug": "npm run watch-js & npm run dev-server & npm run debug-server",
    "debug-server": "devtool lib/server/server --watch",
    "deploy": "npm install --production && npm run build && npm run _deploy_client",
    "_deploy_client": "NODE_ENV=production && node ./node_modules/webpack/bin/webpack.js --color --config ./lib/config/webpack.config.js",
    "update:api": "babel-node src/server/updateAPI"
  },
  "homepage": "https://bitbucket.org/youngshand/react-frontend-2",
  "devDependencies": {
